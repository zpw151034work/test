.dialog .van-button--default {
	width: 544rpx !important;
  height: 80rpx !important;
  line-height: 80rpx;
	margin: 0 auto !important;
	border-radius: 40rpx;
	background: #419D37 !important;
	color: #fff !important;
  margin-bottom: 30rpx !important;
  display:block;
}


.dialogContent {
	width: 100%;
	height: 100%;
	position: relative;
	border: 1rpx solid #fff;
}

.card {
	top: 80rpx;
	width: 400rpx;
	height: 140rpx;
}

.cardText {
	display: inline-block;
	margin-left: 20rpx;
}

.card image {
	width: 82rpx;
	height: 82rpx;
	margin: 40rpx 0 0 30rpx;
	display: inline-block;
}

.card .cardText text:nth-child(2n+1) {
	font-size: 20rpx;
	color: #333;
	display: block;
	width: 100%;
}

.card .cardText text:nth-child(2n) {
	font-size: 24rpx;
	color: #909399;
	display: block;
}

.close {
	position: absolute;
	top: 30rpx;
	right:40rpx;
	width: 40rpx;
	height: 40rpx;
}
.dialogInfo {
