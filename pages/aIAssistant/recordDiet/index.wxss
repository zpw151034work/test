.right-Avatar {
  float: right;
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
 }
.right-Triangle {
  float: right;
  margin-top: 35rpx;
  width: 0;
  height: 0;
  border-top: 16rpx solid transparent;
  border-left: 24rpx solid #64B55B;
  border-bottom: 16rpx solid transparent;
}

.right-message {
  float: right;
  margin-top: 10rpx;
