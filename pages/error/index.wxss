/* pages/error/index.wxss */
.error{
  width: 100vw;
  height: 100vh;
  position: relative;
}
.error .img{
	width:360rpx;
	height:336rpx;
	position:absolute;
	top:50%;
	left:50%;
	transform:translate(-50%,-50%);
}
.error .img image{
	width:360rpx;
	height:336rpx;
}
.button{
  position:absolute;
	top:80%;
	left:50%;
  transform:translate(-50%,-50%);
  width:200rpx;
  text-align: center;
  height:60rpx;
  line-height: 60rpx;
  background:#64B55B ;
}