/* pages/healthassessment/index.wxss */
.container {
	position: absolute;
	width: 750rpx;
	margin: 0 auto;
	box-sizing: border-box;
	overflow: auto;
	background: #fff;
}

.date {
	margin-bottom: 20rpx;
}

/* 标题带方框 */
.content_box {
	width: 702rpx;
	margin: 0 auto;
	background: #fff;
	border-radius: 20rpx;
	margin-bottom: 40rpx;
	border-bottom: 1px solid #eee;
}

.base_info {
	width: 702rpx;
	margin: 0 auto;
}

.dataShow {
	width: 702rpx;
	margin: 0 auto;
	height: 240rpx;
	border: 1rpx solid #eee;
	border-radius: 20rpx;
	margin-bottom: 20rpx;
	z-index: 0;
}

.dataShow .item {
	width: 234rpx;
	text-align: center;
	margin-top: 70rpx;
}

.dataShow .middle {
	width: 230rpx;
	height: 230rpx;
	margin-top: 4rpx;
	position: relative;
}

.dataShow .item text {
	display: block;

}

.textBox {
	position: absolute;
	width: 120rpx;
	height: 120rpx;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.textBox text:nth-child(3n+1) {
	font-size: 20rpx;
	color: #909399;
	display: block;
	text-align: center;
}

.textBox text:nth-child(3n) {
	display: block;
	font-size: 20rpx;
	color: #909399;
	text-align: center;
}

.textBox text:nth-child(3n+2) {
	display: block;
	font-size: 48rpx;
	color: #333;
	text-align: center;
}

.van-image {
	width: 84rpx;
	height: 84rpx;
	border-radius: 10rpx;
}

.cell-slot {
	display: flex;
	border-radius: 10rpx;
	width: 400rpx;
}

.cell-title {
	font-size: 28rpx;
	color: #333;
	font-weight: 600 !important;
}

.cell-reason {
	font-size: 24rpx;
	color: #909399;
}

.bottomNav {
	padding-top: 10rpx;
	width: 750rpx;
	position: fixed;
	bottom: 0rpx;
	left: 50%;
	transform: translate(-50%);
	background: #fff;
	height: 168rpx;
}

.sleepState {
	display: flex;
	width: 662rpx;
	margin: 0 auto;
	align-items: center;
	justify-content: center;
	padding: 0 20rpx;
}

.sleepState .item {
	margin-right: 76rpx;
}

.sleepState .item image {
	width: 48rpx;
	height: 48rpx;
	padding: 0 15rpx;
}

.sleepState .item:nth-child(5n) {
	margin-right: 0rpx !important;
}

.sleepState .item text {
	display: block;
	width: 66rpx;
	font-size: 24rpx;
	color: #666;
	text-align: center;
	padding: 5rpx 0;
}

.delete {
	width: 160rpx;
	height: 100%;
	padding-top: 36rpx;
	line-height: 60rpx;
	color: #fff;
	background: red;
	text-align: center;
}

.van-overlay {
	z-index: 10000 !important;
}

.empty {

	width: 750rpx;

}

.empty .picture {
	width: 204rpx;
	height: 248rpx;
	margin: 0 auto;
	margin-top: 160rpx;
}

.empty .picture image {

	width: 204rpx;
	height: 248rpx;
}

.empty .emptyTextTop {
	width: 146rpx;
	height: 40rpx;
	margin: 0 auto;
	margin-top: 30rpx;
	font-size: 28rpx;
	color: #9B9B9B;
}

.empty .emptyText {
	width: 347rpx;
	height: 40rpx;
	font-size: 28rpx;
	margin: 0 auto;
	color: #9B9B9B;
}

.empty .emptyButtton {
	width: 658rpx;
	margin: 0 auto;
	display: flex;
	justify-content: center;
	margin-top: 186rpx;
}

.empty .emptyButtton view {
	width: 316rpx;
	height: 88rpx;
	line-height: 88rpx;
	text-align: center;
	color: #9B9B9B;
	font-size: 32rpx;
	border-radius: 44rpx;
	background: #F6F6F6;
}

.empty .emptyButtton view:nth-child(2n+1) {
	margin-right: 10rpx;
}

.empty .emptyButtton view:nth-child(2n) {
	margin-left: 10rpx;
}

.tip {
	width: 264rpx;
	height: 34rpx;
	margin: 0 auto;
	margin-top: 20rpx;
	line-height: 34rpx;
	font-size: 24rpx;
	color: #D8D8D8;
}

.custom-icon {
	margin-top: 4rpx !important;
}

/* //时间按钮样式 */
.timeButton {
	width: 702rpx;
	margin: 0 auto;
	height: 50rpx;
}

.timeButtonRight {
	line-height: 60rpx;
	margin-right: 10rpx;
}

.timeButtonCenter {
	width: 20rpx;
	height: 50rpx;
}

.timeButtonLeft {
	height: 50rpx;
}

.van-cell__value {
	font-size: 24rpx !important;
}

/* 页面空格 */
.emptycell {
	display: inline-block;
	width: 48rpx;
}

.vantHeat {
	font-size: 24rpx;
	color: #9B9B9B;
}


.fontWeight {
	font-weight: 600 !important;
}

/* 弹出层 */
.dialog .van-button--default {
	width: 544rpx !important;
	height: 80rpx !important;
	margin: 0 auto !important;
	border-radius: 40rpx;
	background: #419D37 !important;
	color: #fff !important;
	margin-bottom: 30rpx !important;
	line-height: 80rpx;
	display: block;
}

.dialogContent {
	width: 100%;
	height: 100%;
	position: relative;
	border: 1rpx solid #fff;
}

.card {
	top: 80rpx;
	width: 400rpx;
	height: 140rpx;
}

.cardText {
	display: inline-block;
	margin-left: 20rpx;
}

.card image {
	width: 82rpx;
	height: 82rpx;
	margin: 40rpx 0 0 30rpx;
	display: inline-block;
}

.card .cardText text:nth-child(2n+1) {
	font-size: 20rpx;
	color: #333;
	display: block;
	width: 100%;
}

.card .cardText text:nth-child(2n) {
	font-size: 24rpx;
	color: #909399;
	display: block;
}

.close {
	position: absolute;
	top: 30rpx;
	right: 40rpx;
	width: 40rpx;
	height: 40rpx;
}

.dialogInfo {
	widows: 640rpx;
	height: 230rpx;
	display: flex;
