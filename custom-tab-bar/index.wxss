/* 弹出层样式 */
.dialogBox{
  width: 100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  background:#fff;
  z-index:10000;
}
.contentBox {
 position: absolute;
 top:850rpx;
 left:50%;
 transform: translate(-50%,0);
 height:300rpx;
 width: 550rpx;
 display: flex;
 flex-wrap: wrap;
 justify-content: space-between;
 padding:0 100rpx;
 margin-right:110rpx;
}
.contentBox .item{
  width:30%;
  text-align:center; 
}
.contentBox .item image{
 display:block;
 width:110rpx;
 height:110rpx;
 margin-left:28rpx;
}
.contentBox .item text{
  display:block;
  font-size:24rpx;
  color:#333333;
  height:50rpx;
  line-height:50rpx;
}
.close{
 position: absolute;
 top:1250rpx;
 left:50%;
 transform: translate(-50%,0);
 width:50rpx;
 height:50rpx;
 font-size:47rpx;
}
.close image{
  width: 50rpx;
  height: 50rpx;
  display: block;
}
